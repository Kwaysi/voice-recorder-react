{
  "compilerOptions": {
    
    "jsx": "react",
    "strict": true,
    "target": "es5", 
    "outDir": "lib",
    "sourceMap": false,
    "module": "commonjs",
    "declaration": true ,
    "skipLibCheck": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "dom"],
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts", "example"]
}
